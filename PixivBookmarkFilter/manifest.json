{
  "manifest_version": 2,
  "name": "Pixiv Bookmark Filter",
  "version": "0.0.3",
  "description": "pixivの検索結果をブックマーク数でフィルタリングする拡張機能です。",
  "content_scripts": [
    {
      "matches": [
        "http://www.pixiv.net/tags.php?*",
        "http://www.pixiv.net/search.php?*"
      ],
      "js": [ "content.js" ],
      "run_at": "document_end"
    }
  ]
}
